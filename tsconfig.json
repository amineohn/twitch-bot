{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2021"],
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "./",
    "paths": {
        "@/*": ["*"],
        "@events/*": ["events/*"],
        "@repositories/*": ["repositories/*"],
        "@interfaces/*": ["interfaces/*"],
        "@commands/*": ["commands/*"],
        "@libs/*": ["libs/*"],
    },
    "moduleResolution": "node",

  }
}
